<h1>Student Form</h1>
<%= form_tag students_path do %>
    <%= text_field_tag :'student[first_name]', nil, placeholder: 'First Name' %><br>

    <%= text_field_tag :'student[last_name]', nil, placeholder: 'Last Name' %><br>

    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <%= submit_tag "Submit Student" %>
<% end %>